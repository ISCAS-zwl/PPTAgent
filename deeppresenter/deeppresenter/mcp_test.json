[
    {
        "name": "desktop_commander",
        "description": "Sandbox environment for testing",
        "command": "docker",
        "args": [
            "run",
            "--security-opt",
            "seccomp=unconfined",
            "--init",
            "--name",
            "$WORKSPACE_ID",
            "-i",
            "--rm",
            "-v",
            "$HOST_WORKSPACE:$WORKSPACE",
            "-w",
            "$WORKSPACE",
            "deeppresenter-sandbox:0.1.0"
        ]
    }
]
